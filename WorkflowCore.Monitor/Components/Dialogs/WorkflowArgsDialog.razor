<MudDialog>
    <TitleContent>
        <MudText Typo="Typo.h6">
            Arguments:
        </MudText>
    </TitleContent>
    <DialogContent>
        <MudTextField @bind-Value="@Arguments.PrdNr" Label="PrdNr" />
    </DialogContent>
    <DialogActions>
        <MudButton OnClick="Cancel">Cancel</MudButton>
        <MudButton Color="Color.Error" OnClick="Submit">Start</MudButton>
    </DialogActions>
</MudDialog>

@code {
    [CascadingParameter]
    private IMudDialogInstance MudDialog { get; set; } = null!;

    [Parameter]
    public Workflows.StepsProgress Arguments { get; set; } = null!;

    private void Submit() => MudDialog.Close(DialogResult.Ok(Arguments));

    private void Cancel() => MudDialog.Cancel();
}